{
  "suggest.noselect": false,
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.infoSign": "?",
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "javascript",
    "html",
    "css",
    "scss",
    "go",
    "cs"
  ],
  "diagnostic.checkCurrentLine": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "settings": {
        "buildFlags": ["-tags=integration"]
      },
      "rootPatterns": ["go.mod"],
      "filetypes": ["verynotgo"]
    },
    "golangci-lint-languageserver": {
      "command": "golangci-lint-languageserver",
      "filetypes": ["notgo"],
      "initializationOptions": {
        "command": [
          "golangci-lint",
          "run",
          "--out-format",
          "json",
          "--issues-exit-code=1"
        ]
      }
    },
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["notcs"],
      "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
    }
  },
  "go.goplsOptions": {
    "staticcheck": false,
    "buildFlags": ["-tags=apptest,integration,apitest"]
  },
  "go.goplsPath": "/Users/patrick.white/go/bin/gopls",
  "go.goplsEnv": {
    "GOFLAGS": "-tags=apptest,integration,apitest"
  }
}
